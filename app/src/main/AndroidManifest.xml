    package="com.example.app_kesehatan">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baktihusada"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MenuUtama" />
        <activity android:name=".formIsiData" />
        <activity android:name=".DataKesehatan" />
        <activity android:name=".inputBerhasil" />
        <activity android:name=".lihatKecamatan" />
        <activity android:name=".LihatKelurahan" />
        <activity android:name=".LihatKeluarga" />
        <activity android:name=".EditKeluarga" />
    </application>

</manifest>